package com.kh.ch09_interface;

public abstract class Monstera extends Plant {
	
	public Monstera(String name) {
		this.name = name;	//super(name);
	}
	
	@Override
	public void sprinkleWater() {
		// nutrients의 값을 10 증가
		int n = getNutrients();
		setNutrients(n+10);
	}
	
	@Override
	public void baskSun() {
		// nutreints의 값을 20 증가
		setNutrients(getNutrients() + 20);	
	}

}
